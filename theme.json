{
	"$schema": "https://schemas.wp.org/wp/6.8/theme.json",
	"version": 3,
	"settings": {
		"appearanceTools": true,
		"useRootPaddingAwareAlignments": false,
		"spacing": {
			"units": ["%", "px", "em", "rem", "vh", "vw"],
			"customSpacingSize": true,
			"spacingScale": {
				"operator": "*",
				"increment": 1.5,
				"steps": 7,
				"mediumStep": 1.5,
				"unit": "rem"
			},
			"spacingSizes": [
				{
					"size": "0.5rem",
					"slug": "small",
					"name": "Small"
				},
				{
					"size": "1rem",
					"slug": "medium",
					"name": "Medium"
				},
				{
					"size": "1.5rem",
					"slug": "large",
					"name": "Large"
				},
				{
					"size": "2rem",
					"slug": "x-large",
					"name": "Extra Large"
				}
			]
		},
		"color": {
			"customDuotone": true,
			"customGradient": true,
			"defaultDuotone": false,
			"defaultGradients": false,
			"defaultPalette": false,
			"palette": [
				{
					"color": "#000000",
					"name": "Asari Black",
					"slug": "asari-black"
				},
				{
					"color": "#FFFFFF",
					"name": "Asari White",
					"slug": "asari-white"
				},
				{
					"color": "#F8F8F8",
					"name": "Asari Soft Gray",
					"slug": "asari-soft-gray"
				},
				{
					"color": "#F1F1F1",
					"name": "Asari Warm Gray",
					"slug": "asari-warm-gray"
				},
				{
					"color": "#CCCCCC",
					"name": "Asari Gray",
					"slug": "asari-gray"
				},
				{
					"color": "#B8CBE5",
					"name": "Asari Blue",
					"slug": "asari-blue"
				},
				{
					"color": "#BE9D64",
					"name": "Asari Gold",
					"slug": "asari-gold"
				},
				{
					"color": "#9D7D46",
					"name": "Asari Deep Gold",
					"slug": "asari-deep-gold"
				}
			],
			"gradients": [
				{
					"gradient": "linear-gradient(135deg, #BE9D64 0%, #9D7D46 100%)",
					"name": "Gold Gradient",
					"slug": "gold-gradient"
				},
				{
					"gradient": "linear-gradient(135deg, #F8F8F8 0%, #F1F1F1 100%)",
					"name": "Gray Gradient",
					"slug": "gray-gradient"
				}
			]
		},
		"typography": {
			"customFontSize": true,
			"fontStyle": true,
			"fontWeight": true,
			"letterSpacing": true,
			"lineHeight": true,
			"textDecoration": true,
			"textTransform": true,
			"dropCap": false,
			"fontFamilies": [
				{
					"fontFamily": "\"CoFo Gothic Trial\", -apple-system, BlinkMacSystemFont, \"Segoe UI\", Roboto, Oxygen-Sans, Ubuntu, Cantarell, \"Helvetica Neue\", sans-serif",
					"name": "CoFo Gothic Trial",
					"slug": "cofo-gothic"
				},
				{
					"fontFamily": "\"Manege Trial\", Georgia, \"Times New Roman\", Times, serif",
					"name": "Manege Trial",
					"slug": "manege"
				}
			],
			"fontSizes": [
				{
					"size": "12px",
					"slug": "b-5",
					"name": "Body 5 (12px)"
				},
				{
					"size": "14px",
					"slug": "b-4",
					"name": "Body 4 (14px)"
				},
				{
					"size": "16px",
					"slug": "b-3",
					"name": "Body 3 (16px)"
				},
				{
					"size": "22px",
					"slug": "b-2",
					"name": "Body 2 (22px)"
				},
				{
					"size": "24px",
					"slug": "b-1",
					"name": "Body 1 (24px)"
				},
				{
					"size": "32px",
					"slug": "h-3",
					"name": "Heading 3 (32px)"
				},
				{
					"size": "48px",
					"slug": "h-2",
					"name": "Heading 2 (48px)"
				},
				{
					"size": "110px",
					"slug": "h-1",
					"name": "Heading 1 (110px)"
				}
			]
		},
		"border": {
			"color": true,
			"radius": true,
			"style": true,
			"width": true
		},
		"shadow": {
			"defaultPresets": false,
			"presets": [
				{
					"name": "Soft Shadow",
					"slug": "soft",
					"shadow": "0 4px 16px rgba(184, 203, 229, 0.15)"
				},
				{
					"name": "Medium Shadow",
					"slug": "medium",
					"shadow": "0 8px 32px rgba(184, 203, 229, 0.25)"
				}
			]
		}
	},
	"styles": {
		"color": {
			"background": "var(--wp--preset--color--asari-white)",
			"text": "var(--wp--preset--color--asari-black)"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--cofo-gothic)",
			"fontSize": "var(--wp--preset--font-size--b-3)",
			"fontWeight": "200",
			"lineHeight": "1.5"
		},
		"spacing": {
			"blockGap": "1.5rem"
		},
		"elements": {
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--cofo-gothic)",
					"fontWeight": "200",
					"lineHeight": "1.2"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--h-1)",
					"fontWeight": "200"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--h-2)",
					"fontWeight": "200"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--h-3)",
					"fontWeight": "200"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--asari-gold)"
				},
				"typography": {
					"textDecoration": "none"
				},
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--asari-deep-gold)"
					},
					"typography": {
						"textDecoration": "underline"
					}
				}
			},
			"button": {
				"color": {
					"background": "var(--wp--preset--color--asari-white)",
					"text": "var(--wp--preset--color--asari-black)"
				},
				"border": {
					"color": "var(--wp--preset--color--asari-warm-gray)",
					"radius": "30px",
					"style": "solid",
					"width": "1px"
				},
				"spacing": {
					"padding": {
						"top": "10px",
						"right": "14px",
						"bottom": "10px",
						"left": "14px"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--cofo-gothic)",
					"fontSize": "var(--wp--preset--font-size--b-2)",
					"fontWeight": "200",
					"lineHeight": "1"
				},
				":hover": {
					"color": {
						"background": "var(--wp--preset--color--asari-soft-gray)"
					}
				}
			}
		},
		"blocks": {
			"core/site-title": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--cofo-gothic)",
					"fontSize": "var(--wp--preset--font-size--h-3)",
					"fontWeight": "200"
				},
				"color": {
					"text": "var(--wp--preset--color--asari-black)"
				},
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--asari-black)"
						},
						":hover": {
							"color": {
								"text": "var(--wp--preset--color--asari-gold)"
							}
						}
					}
				}
			},
			"core/site-tagline": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--b-4)",
					"fontWeight": "200"
				},
				"color": {
					"text": "var(--wp--preset--color--asari-gray)"
				}
			},
			"core/navigation": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--b-3)",
					"fontWeight": "200"
				},
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--asari-black)"
						},
						":hover": {
							"color": {
								"text": "var(--wp--preset--color--asari-gold)"
							}
						}
					}
				}
			},
			"core/post-title": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--cofo-gothic)",
					"fontSize": "var(--wp--preset--font-size--h-2)",
					"fontWeight": "200"
				},
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--asari-black)"
						},
						":hover": {
							"color": {
								"text": "var(--wp--preset--color--asari-gold)"
							}
						}
					}
				}
			},
			"core/post-content": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--b-3)",
					"lineHeight": "1.6"
				}
			},
			"core/post-date": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--b-4)",
					"fontWeight": "200"
				},
				"color": {
					"text": "var(--wp--preset--color--asari-gray)"
				}
			},
			"core/post-author": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--b-4)",
					"fontWeight": "200"
				},
				"color": {
					"text": "var(--wp--preset--color--asari-gray)"
				}
			},
			"core/post-terms": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--b-5)",
					"fontWeight": "200"
				},
				"color": {
					"text": "var(--wp--preset--color--asari-gray)"
				},
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--asari-gold)"
						},
						":hover": {
							"color": {
								"text": "var(--wp--preset--color--asari-deep-gold)"
							}
						}
					}
				}
			},
			"core/separator": {
				"color": {
					"background": "var(--wp--preset--color--asari-warm-gray)"
				}
			}
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header",
			"title": "Header"
		},
		{
			"area": "footer",
			"name": "footer",
			"title": "Footer"
		}
	],
	"customTemplates": [
		{
			"name": "single-practice",
			"title": "Practice Area",
			"postTypes": ["practice"]
		},
		{
			"name": "single-employee",
			"title": "Employee Profile",
			"postTypes": ["employee"]
		},
		{
			"name": "single-event",
			"title": "Event",
			"postTypes": ["event"]
		},
		{
			"name": "single-vacancy",
			"title": "Vacancy",
			"postTypes": ["vacancy"]
		},
		{
			"name": "single-office",
			"title": "Office",
			"postTypes": ["office"]
		}
	]
}